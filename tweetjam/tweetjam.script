local checkerboard_spiral = [[
t=0
w=cos
z=sin
::_::
cls()
t=t+.01
e=40*w(t)
d=40*z(t)
for b=0,1,.001 do
	l=80*b
	m=l/2-t/2
	k=l*z(m)
	j=l*w(m)
	o,p=k+e,j+d
	s=w(o/50+t)*z(p/50+t)
	circ(64+k-s,64+j-s,1+2*s,8+l/4%3)
end
flip()
goto _]]

